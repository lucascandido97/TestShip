{
  "name": "web-app-backend",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "scripts": {
    "start": "node dist/bundle.js",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/app.ts",
    "build": "tsc --noEmit && node esbuild.config.js",
    "test": "jest",
    "test:unit": "jest --testMatch '**/tests/unit/**/*.test.ts'",
    "test:integration": "jest --testMatch '**/tests/integration/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/supertest": "^6.0.3",
    "esbuild": "^0.25.1",
    "jest": "^29.7.0",
    "supertest": "^6.1.6",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.5.4",
    "eslint": "^8.39.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0"
  }
}
